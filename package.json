{
  "name": "iobroker.ochsner-roomterminal",
  "version": "0.0.6",
  "description": "Interface to the Ochsner Roomterminal Webserver (web2comm)",
  "author": {
    "name": "Armin Stegerer",
    "email": "Armin.Stegerer@t-online.de"
  },
  "homepage": "https://github.com/Armin Stegerer/ioBroker.ochsner-roomterminal",
  "license": "MIT",
  "keywords": [
    "ochsner",
    "heatpump",
    "web2comm"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Armin Stegerer/ioBroker.ochsner-roomterminal.git"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.7",
    "digest-fetch": "^2.0.3",
    "node-fetch": "^2.6.7",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.9",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/adapter-react-v5": "^3.1.34",
    "@iobroker/testing": "^4.1.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@mui/styles": "^5.10.9",
    "@tsconfig/node16": "^1.0.3",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "mocha": "^10.2.0",
    "prettier": "^2.8.3",
    "proxyquire": "^2.1.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "translate": "translate-adapter",
    "release": "release-script patch --yes",
    "release-minor": "release-script minor --yes",
    "release-major": "release-script major --yes"
  },
  "bugs": {
    "url": "https://github.com/Armin Stegerer/ioBroker.ochsner-roomterminal/issues"
  },
  "readmeFilename": "README.md"
}
